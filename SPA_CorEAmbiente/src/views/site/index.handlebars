    <section id="produtos-servicos" class="mt-2 mb-4">
        <div class="container">
            <div class="row">
                <div class="col-md-8" id="produtos">
                    <h4>Produtos e Serviços</h4>
                    {{#each produtos}}
                    <hr>
                    <div class="card mb-3">
                        <div class="row g-0">
                            <div class="col-md-4">
                                <img src="http://localhost:3000/img/produtos/{{imagem_principal}}"
